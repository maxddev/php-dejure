{
    "name": "s1syphos/php-dejure",
    "description": "Linking texts with dejure.org, the Class(y) way.",
    "type": "library",
    "license": "MIT",
    "version": "1.2.0",
    "keywords": ["api", "legal", "dejure", "dejure.org"],
    "homepage": "https://github.com/S1SYPHOS",
    "scripts": {
        "cs-dry": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix",
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Martin Folkers",
            "homepage": "https://twobrain.io",
            "role": "Maintainer"
        }
    ],
    "require": {
        "shieldon/simple-cache": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "S1SYPHOS\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "S1SYPHOS\\Tests\\": "tests/"
        }
    }
}
